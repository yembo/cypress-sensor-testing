{
  "baseUrl": "http://localhost:8080",
  "defaultCommandTimeout": 5000,
  "pageLoadTimeout": 5000,
  "requestTimeout": 5000,
  "watchForFileChanges":false,
  "env": {
    "browserPermissions": {
      "notifications": "allow",
      "geolocation": "allow",
      "camera": "allow",
      "microphone": "allow",
      "images": "allow",
      "javascript": "allow",
      "popups": "allow",
      "plugins": "ask",
      "cookies": "allow"
    }
  },
  "retries": {
    "runMode": 2
  },
  "numTestsKeptInMemory": 1,
  "viewportWidth": 540,
  "viewportHeight": 1200,
  "testFiles": "**/*.spec.ts"
}
